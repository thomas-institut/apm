<TEI>
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <editor xml:id="someusername"/>
            </titleStmt>
        </fileDesc>
    </teiHeader>
    <text xml:lang='la'>
        <body>
            <div type="page" facs="SOME_PAGE_ID">
                <!-- Line 1 -->
                <l>Latine</l>
                <!-- Line 2 -->
                <l xml:lang='ar'>Arabicae</l>
                <fw type="whatever">Not a custodes, ignore</fw>
                <head>A heading</head>
                <fw type="catch">A custodes</fw>
                <!-- Line breaks within a line will be converted to a 
                     single space! -->
                <!-- Line 3 -->
                <l>Another line 
                    with <sic>Some text</sic>
                </l>
                <bad>Not valid element, ignore</bad>
                <!-- Gap for lines 4 and 5 -->
                <gap reason="pending-transcription" quantity="2" unit="line"/>
                <note type="gloss">A gloss</note>
                <!-- Line 6! -->
                <l>Yet another line <metamark xml:id="page-XXX-add-YYY"/></l>
                <fw type="pageNum">175v</fw>
                <add place="margin left" target="page-XXX-add-YYY">
                    An addition
                </add>
            </div>
        </body>
    </text>
</TEI>

