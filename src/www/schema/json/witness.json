{
  "$id": "witness.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A witness",
  "type": "object",
  "properties": {
    "chunkId" : {
      "type": "string"
    },
    "witnessType": {
      "type": "string"
    },
    "tokens": {
      "type": "array",
      "items": {
        "$ref": "token.json"
      }
    }
  },
  "required": [
    "witnessType",
    "chunkId",
    "tokens"
  ]
}