{% extends "normal-page.twig" %}

{% block extraCSS %}
    <link rel="stylesheet" type="text/css" href="{{baseUrl}}/css/doc_page.css"/>
{% endblock %}

{% block extraJS %}
<script type="application/javascript" src="{{baseUrl}}/js/dist/DocDefPages.bundle.js"></script>
<script>
    $( () => {
        new DocDefPages( {
          commonData:  JSON.parse("{{commonData|json_encode|e('js')}}"),
          doc: JSON.parse("{{doc|json_encode|e('js')}}"),
          numPages: {{doc.numPages}},
        });
    })
</script>
{% endblock %}

