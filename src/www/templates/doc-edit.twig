{% extends "normal-page.twig" %}

{% block title %}Edit Doc: {{docInfo.title}}{% endblock %}

{% block extraJS %}
<script type="application/javascript" src="{{baseUrl}}/js/dist/DocEditPage.bundle.js"></script>
<script>
    $( () => {
      new DocEditPage({
        commonData: JSON.parse("{{ commonData|json_encode|e('js') }}"),
        docInfo: JSON.parse('{{ docInfo|json_encode|e('js') }}'),
        languages: JSON.parse('{{ languages|json_encode|e('js') }}'),
        docTypes: JSON.parse('{{ docTypes|json_encode|e('js') }}'),
        imageSources: JSON.parse('{{ imageSources|json_encode|e('js') }}'),
        canBeDeleted: JSON.parse('{{ canBeDeleted|json_encode|e('js') }}')
      })
    });

</script>
{% endblock %}
