{% extends "normal-page.twig" %}

{% block title %}{% endblock %}

{% block extraCSS %}
    <link rel="stylesheet" type="text/css" href="{{baseUrl}}/css/entity.css"/>
{% endblock %}

{% block extraJS %}
    <script type="application/javascript" src="{{baseUrl}}/js/dist/EntityPage.bundle.js"></script>
{% endblock %}


{% block finalBodyJS %}
    <script>

      $(() => {
        new EntityPage(( {
          commonData:  JSON.parse("{{commonData|json_encode|e('js')}}"),
          entityData: JSON.parse("{{entityData|json_encode|e('js')}}"),
        }))
      })
    </script>
{%  endblock %}
