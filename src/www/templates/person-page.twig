{% extends "normal-page.twig" %}

{% block title %}Person{% endblock %}

{% block extraCSS %}
    <link rel="stylesheet" type="text/css" href="{{baseUrl}}/css/person_page.css"/>
{% endblock %}

{% block extraJS %}
    <script type="application/javascript" src="{{baseUrl}}/js/dist/PersonPage.bundle.js"></script>
{% endblock %}


{% block finalBodyJS %}
    <script>
      $(() => {
        new PersonPage(( {
          commonData:  JSON.parse("{{commonData|json_encode|e('js')}}"),
          personData: JSON.parse("{{personData|json_encode|e('js')}}"),
          canManageUsers: JSON.parse("{{canManageUsers|json_encode|e('js')}}"),
        }))
      })
    </script>
{%  endblock %}


