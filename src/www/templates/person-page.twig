{% extends "normal-page.twig" %}

{% block title %}Person{% endblock %}

{% block extraCSS %}
    <link rel="stylesheet" type="text/css" href="{{baseurl}}/css/person_page.css"/>
{% endblock %}

{% block extraJS %}
    <script type="application/javascript" src="{{baseurl}}/js/dist/PersonPageNew.bundle.js"></script>
{% endblock %}


{% block finalBodyJS %}
    <script>
      $(() => {
        new PersonPageNew(( {
          commonData:  JSON.parse("{{commonData|json_encode|e('js')}}"),
          tid: {{ tid }},
          data: JSON.parse("{{data|json_encode|e('js')}}")
        }))
      })
    </script>
{%  endblock %}


